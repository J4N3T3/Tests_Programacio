#include <stdio.h>
#include <string.h>

int main() {
	int val1 = 0, val2 = 0, val3 = 0;
		
	struct dades
	{
		int val1, val2, val3;
	};
	struct dades valors;
	valors.val1 = 4;
	valors.val2 = 3;
	valors.val3 = 1;
	scanf_s("%d", &valors.val1);
	scanf_s("%d", &valors.val2);
	scanf_s("%d", &valors.val3);

	printf("%d\n", valors.val1);
	printf("%d\n", valors.val2);
	printf("%d\n", valors.val3);

}
