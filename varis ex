#include <stdio.h>

void negativo() {
	int n1;
	printf("Ingrese un numero:\n\n");
	scanf_s("%d", &n1);
	if (n1 >= 0)
	{
		printf("\n0");
	}
	else
	{
		printf("\n1");
	}
}

int main() {
	negativo();
}
