/*
#include <stdio.h>

void media3() {
	int n1, n2, n3;
	float result;
	printf("Ingrese tres numeros:\n\n");
	scanf_s("%d %d %d", &n1, &n2, &n3);
	result = (n1 + n2 + n3) / 3;
	printf("\nLa media de los tres numeros es: %f", result);
}

int main() {
	media3();
}


